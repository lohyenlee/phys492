Cell[TextData[{
 "For a spin-half spin, we may write the unitary time-evolution \
operator\.08in the standard representation as\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox[
     UnderscriptBox[
      StyleBox["U",
       FontWeight->"Bold",
       FontSlant->"Plain"], "_"], "_"], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         RowBox[{"cos", " ", 
          FractionBox["\[Theta]", "2"]}], "-", 
         RowBox[{
          FractionBox[
           RowBox[{"i", "\[InvisibleSpace]", 
            SubscriptBox["\[Theta]", "z"]}], "\[Theta]"], "sin", " ", 
          FractionBox["\[Theta]", "2"]}]}], 
        RowBox[{
         FractionBox[
          RowBox[{
           RowBox[{"-", 
            SubscriptBox["\[Theta]", "y"]}], "-", 
           RowBox[{"i", "\[InvisibleSpace]", 
            SubscriptBox["\[Theta]", "x"]}]}], "\[Theta]"], "sin", " ", 
         FractionBox["\[Theta]", "2"]}]},
       {
        RowBox[{
         FractionBox[
          RowBox[{
           SubscriptBox["\[Theta]", "y"], "-", 
           RowBox[{"i", "\[InvisibleSpace]", 
            SubscriptBox["\[Theta]", "x"]}]}], "\[Theta]"], "sin", " ", 
         FractionBox["\[Theta]", "2"]}], 
        RowBox[{
         RowBox[{"cos", " ", 
          FractionBox["\[Theta]", "2"]}], "+", 
         RowBox[{
          FractionBox[
           RowBox[{"i", "\[InvisibleSpace]", 
            SubscriptBox["\[Theta]", "z"]}], "\[Theta]"], "sin", " ", 
          FractionBox["\[Theta]", "2"]}]}]}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7710fece-c73b-462e-97da-f907e35d6e1d"],
 "."
}], "Text",
 CellChangeTimes->{{3.896642649786152*^9, 
