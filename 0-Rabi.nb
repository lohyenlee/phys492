       FractionBox[
        RowBox[{"V", "/", "2"}], 
        RowBox[{"\[CapitalOmega]", "+", 
         RowBox[{"\[Delta]", "/", "2"}]}]], "+", 
       FractionBox[
        RowBox[{"V", "/", "2"}], 
        RowBox[{"\[CapitalOmega]", "-", 
