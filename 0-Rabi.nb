Cell[TextData[{
 "If one starts with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"\[LeftBracketingBar]", "\[CapitalPsi]"}], 
        "\[RightAngleBracket]"}], "=", 
       RowBox[{"\[LeftBracketingBar]", 
        RowBox[{"\[UpArrow]", "\[UpArrow]"}]}]}], "\[RightAngleBracket]"}], 
     "=", 
     RowBox[{"\[LeftBracketingBar]", "00"}]}], "\[RightAngleBracket]"}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b9cfda4e-62b1-48a7-9f63-8854882a3ba9"],
 ", then nothing happens to the state."
}], "Text",
 CellChangeTimes->{{3.8985963293830147`*^9, 3.898596334407352*^9}, {
  3.898596556415056*^9, 
  3.898596580322566*^9}},ExpressionUUID->"e1c4a51d-4a0d-4937-829a-\
31e33cd9155e"],

Cell[TextData[{
 "But if one starts with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"\[LeftBracketingBar]", "\[CapitalPsi]"}], 
        "\[RightAngleBracket]"}], "=", 
       RowBox[{"\[LeftBracketingBar]", 
        RowBox[{"\[UpArrow]", "\[DownArrow]"}]}]}], "\[RightAngleBracket]"}], 
     "=", 
     RowBox[{"\[LeftBracketingBar]", "01"}]}], "\[RightAngleBracket]"}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "67ca7678-cff1-4d94-a548-a76f12a5bd10"],
 ", then\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox[
      StyleBox["\[CapitalPsi]",
       FontWeight->"Bold",
       FontSlant->"Plain"], "_"], "(", "t", ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox[
          RowBox[{"i", "\[InvisibleSpace]", "\[Theta]"}], "2"]}], 
        RowBox[{
         SubscriptBox[
          UnderscriptBox[
           UnderscriptBox[
            StyleBox["S",
             FontWeight->"Bold",
             FontSlant->"Plain"], "_"], "_"], "1"], 
         StyleBox["\[CenterDot]",
          FontWeight->"Bold"], 
         SubscriptBox[
          UnderscriptBox[
           UnderscriptBox[
            StyleBox["S",
             FontWeight->"Bold",
             FontSlant->"Plain"], "_"], "_"], "2"]}]}], ")"}], 
      RowBox[{
       UnderscriptBox[
        StyleBox["\[CapitalPsi]",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "(", "0", ")"}]}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {"0"},
         {
          RowBox[{
           SuperscriptBox["e", 
            RowBox[{"i", "\[InvisibleSpace]", 
             RowBox[{"\[Theta]", "/", "2"}]}]], "cos", " ", "\[Theta]"}]},
         {
          RowBox[{
           RowBox[{"-", "i"}], "\[InvisibleSpace]", 
           SuperscriptBox["e", 
            RowBox[{"i", "\[InvisibleSpace]", 
             RowBox[{"\[Theta]", "/", "2"}]}]], "sin", " ", "\[Theta]"}]},
         {"0"}
        }], ")"}], "\[Proportional]", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"cos", " ", "\[Theta]"}], ")"}], Cell[
        "\[LeftBracketingBar]01\[RightAngleBracket]",ExpressionUUID->
         "58c61644-b500-4bbd-bfa9-05ca8ea6ec77"]}], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"i", " ", "sin", " \[InvisibleSpace]", "\[Theta]"}], ")"}], 
        Cell["\[LeftBracketingBar]10\[RightAngleBracket]",ExpressionUUID->
         "902fbba8-8aff-4d39-a033-944ce2ffb430"]}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"88639479-3dd5-471f-a9eb-96946c3dfec5"],
 ".\nFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Theta]", "=", 
    FractionBox["\[Pi]", "4"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d681fab9-a67b-4639-8480-aa35d17a9d30"],
 " we have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderscriptBox[
      StyleBox["\[CapitalPsi]",
       FontWeight->"Bold",
       FontSlant->"Plain"], "_"], "(", "t", ")"}], "=", 
