  FormBox[
   RowBox[{"=", 
    RowBox[{
     FractionBox["i", "\[HBar]"], "J", 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       SubscriptBox[
        OverscriptBox["S", "^"], 
        RowBox[{"1", "\[InvisibleSpace]", "j"}]], "i", "\[InvisibleSpace]", 
