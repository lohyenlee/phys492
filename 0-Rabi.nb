Cell[TextData[{
 "Another way is guess a solution whose time dependence is ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    RowBox[{
     RowBox[{"-", "i"}], "\[InvisibleSpace]", "\[CapitalOmega]", 
     "\[InvisibleSpace]", "t"}]], TraditionalForm]],ExpressionUUID->
  "545380b9-5379-4567-9fa2-e45a663d487f"],
 ":\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["C", "\[UpArrow]"], "(", "t", ")"}], "=", 
    RowBox[{"A", "\[InvisibleSpace]", 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "i"}], "\[InvisibleSpace]", "\[CapitalOmega]", 
       "\[InvisibleSpace]", "t"}]]}]}], TraditionalForm]],ExpressionUUID->
  "686d526c-aa99-4ced-8777-cf2b989de389"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["C", "\[DownArrow]"], "(", "t", ")"}], "=", 
    RowBox[{"B", "\[InvisibleSpace]", 
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "i"}], "\[InvisibleSpace]", "\[CapitalOmega]", 
       "\[InvisibleSpace]", "t"}]]}]}], TraditionalForm]],ExpressionUUID->
  "20830273-8e50-4966-9ac7-f6dc96179f20"],
 ".\nThen we obtain\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[CapitalOmega]", "+", 
       FractionBox["\[Delta]", "2"]}], ")"}], "\[InvisibleSpace]", "A"}], "=", 
    RowBox[{
     FractionBox["V", "2"], " ", "B"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8838c329-ecbb-4b1a-a528-958c2dd762dd"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"\[CapitalOmega]", "-", 
       FractionBox["\[Delta]", "2"]}], ")"}], "\[InvisibleSpace]", "B"}], "=", 
    RowBox[{
     FractionBox["V", "2"], " ", "A"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ace771f5-3201-4299-97cb-f766b672b9e0"],
 "\n\[Therefore]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["B", "A"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"\[CapitalOmega]", "+", 
       FractionBox["\[Delta]", "2"]}], 
      FractionBox["V", "2"]], "=", 
     FractionBox[
      FractionBox["V", "2"], 
      RowBox[{"\[CapitalOmega]", "-", 
       FractionBox["\[Delta]", "2"]}]]}]}], TraditionalForm]],ExpressionUUID->
  "e7dda63b-abfd-43d1-b14d-fba2675598c0"],
 "\n\[Therefore]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[CapitalOmega]", "2"], "-", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["\[Delta]", "2"], ")"}], "2"]}], "=", 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox["V", "2"], ")"}], "2"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "48792228-7bee-451d-a96f-f2c363dd3d45"],
 "\n\[Therefore]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[CapitalOmega]", "=", 
