1dfcb99dcbc7"],

Cell[TextData[{
 "From Ehrenfest\[CloseCurlyQuote]s theorem,\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", 
      RowBox[{"d", "\[InvisibleSpace]", "t"}]], 
     RowBox[{"\[LeftAngleBracket]", 
      UnderscriptBox[
       OverscriptBox[
        StyleBox["S",
         FontWeight->"Bold",
         FontSlant->"Plain"], "^"], "_"], "\[RightAngleBracket]"}]}], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"i", "\[InvisibleSpace]", "\[HBar]"}]], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"[", 
       RowBox[{
        OverscriptBox[
         StyleBox["S",
          FontWeight->"Bold",
          FontSlant->"Plain"], "^"], ",", 
        OverscriptBox["H", "^"]}], "]"}], "\[RightAngleBracket]"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7879081d-8e44-486f-a618-0fbb65da8216"],
 "\n\t\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["i", "\[HBar]"]}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"[", 
       RowBox[{
        OverscriptBox[
         StyleBox["S",
          FontWeight->"Bold",
          FontSlant->"Plain"], "^"], ",", 
        RowBox[{
         UnderscriptBox[
          StyleBox["\[Omega]",
           FontWeight->"Bold",
           FontSlant->"Plain"], "_"], "\[CenterDot]", 
         UnderscriptBox[
          OverscriptBox[
           StyleBox["S",
            FontWeight->"Bold",
            FontSlant->"Plain"], "^"], "_"]}]}], "]"}], 
      "\[RightAngleBracket]"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c62951b7-48c0-45b4-b97c-106e76216814"],
 "\n\[Therefore]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", 
      RowBox[{"d", "\[InvisibleSpace]", "t"}]], 
     RowBox[{"\[LeftAngleBracket]", 
      SubscriptBox[
       OverscriptBox["S", "^"], "i"], "\[RightAngleBracket]"}]}], "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["i", "\[HBar]"]}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"[", 
       RowBox[{
        SubscriptBox[
         OverscriptBox["S", "^"], "i"], ",", 
        RowBox[{
         SubscriptBox["\[Omega]", "j"], 
         SubscriptBox[
          OverscriptBox["S", "^"], "j"]}]}], "]"}], 
      "\[RightAngleBracket]"}]}]}], TraditionalForm]],
  InitializationCell->True,
  FormatType->TraditionalForm,ExpressionUUID->
  "f9efe2de-c265-4dd9-9ddf-f2e8aa366c4e"],
 "\n\t\t   ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["i", "\[HBar]"]}], "\[InvisibleSpace]", 
     SubscriptBox["\[Omega]", "j"], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"[", 
       RowBox[{
        SubscriptBox[
         OverscriptBox["S", "^"], "i"], ",", 
        SubscriptBox[
         OverscriptBox["S", "^"], "j"]}], "]"}], "\[RightAngleBracket]"}]}]}],
    TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e4062c1f-079b-46e3-8ccc-919c0cd40603"],
 "\n\t\t   ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["i", "\[HBar]"]}], "\[InvisibleSpace]", 
     SubscriptBox["\[Omega]", "j"], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"i", "\[InvisibleSpace]", 
       SubscriptBox["\[HBar]\[CurlyEpsilon]", 
        RowBox[{"i", "\[InvisibleSpace]", "j", "\[InvisibleSpace]", "k"}]], 
       SubscriptBox[
        OverscriptBox["S", "^"], "k"]}], "\[RightAngleBracket]"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fdbdb94e-b2b1-4d45-b2c3-279dd3eae316"],
 "\n\t\t   ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     SubscriptBox["\[CurlyEpsilon]", 
      RowBox[{"i", "\[InvisibleSpace]", "j", "\[InvisibleSpace]", "k"}]], 
     SubscriptBox["\[Omega]", "j"], 
     RowBox[{"\[LeftAngleBracket]", 
      SubscriptBox[
       OverscriptBox["S", "^"], "k"], "\[RightAngleBracket]"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9b8bada9-1a5a-4031-85f4-9852c41ebff9"],
 "\n\[Therefore]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", 
      RowBox[{"d", "\[InvisibleSpace]", "t"}]], 
     RowBox[{"\[LeftAngleBracket]", 
      UnderscriptBox[
       OverscriptBox[
        StyleBox["S",
         FontWeight->"Bold",
         FontSlant->"Plain"], "^"], "_"], "\[RightAngleBracket]"}]}], "=", 
    RowBox[{
     UnderscriptBox[
      StyleBox["\[Omega]",
       FontWeight->"Bold",
       FontSlant->"Plain"], "_"], "\[Times]", 
     RowBox[{"\[LeftAngleBracket]", 
      UnderscriptBox[
       OverscriptBox[
        StyleBox["S",
         FontWeight->"Bold",
         FontSlant->"Plain"], "^"], "_"], "\[RightAngleBracket]"}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6d5104a2-c251-43cf-9dd0-0141d3bd03ba"],
 "\nwhich is identical to the classical result."
}], "Text",
 CellChangeTimes->{{3.896642690736052*^9, 3.896642915578961*^9}, {
  3.8966430701069393`*^9, 3.896643077503173*^9}, {3.896643351947967*^9, 
