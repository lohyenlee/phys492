Cell[TextData[{
 "From Ehrenfest\[CloseCurlyQuote]s theorem,\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", 
      RowBox[{"d", "\[InvisibleSpace]", "t"}]], "\[InvisibleSpace]", 
     RowBox[{"\[LeftAngleBracket]", 
      SubscriptBox[
       UnderscriptBox[
        OverscriptBox[
         StyleBox["S",
          FontWeight->"Bold",
          FontSlant->"Plain"], "^"], "_"], "2"], "\[RightAngleBracket]"}]}], 
    "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"i", "\[InvisibleSpace]", "\[HBar]"}]], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"[", 
       RowBox[{
        SubscriptBox[
         OverscriptBox[
          StyleBox["S",
           FontWeight->"Bold",
           FontSlant->"Plain"], "^"], "2"], ",", 
        OverscriptBox["H", "^"]}], "]"}], "\[RightAngleBracket]"}]}]}], 
   TraditionalForm]],ExpressionUUID->"78349e48-6b0e-43da-b8ab-c111d0924d9a"],
 "\n\t\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["i", "\[HBar]"]}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"[", 
       RowBox[{
        SubscriptBox[
         OverscriptBox[
          StyleBox["S",
           FontWeight->"Bold",
           FontSlant->"Plain"], "^"], "2"], ",", 
        RowBox[{
         RowBox[{"-", "J"}], 
         RowBox[{
          SubscriptBox[
           UnderscriptBox[
            OverscriptBox[
             StyleBox["S",
              FontWeight->"Bold",
              FontSlant->"Plain"], "^"], "_"], "1"], "\[CenterDot]", 
          SubscriptBox[
           UnderscriptBox[
            OverscriptBox[
             StyleBox["S",
              FontWeight->"Bold",
              FontSlant->"Plain"], "^"], "_"], "2"]}]}]}], "]"}], 
      "\[RightAngleBracket]"}]}]}], TraditionalForm]],ExpressionUUID->
  "d122593d-3f5f-40c0-b455-cf4af985f172"],
 "\n\[Therefore]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["d", 
      RowBox[{"d", "\[InvisibleSpace]", "t"}]], "\[InvisibleSpace]", 
     RowBox[{"\[LeftAngleBracket]", 
      SubscriptBox[
       OverscriptBox["S", "^"], 
       RowBox[{"2", "\[InvisibleSpace]", "i"}]], "\[RightAngleBracket]"}]}], 
    "=", 
    RowBox[{
     RowBox[{"-", 
      FractionBox["i", "\[HBar]"]}], 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{"[", 
       RowBox[{
        SubscriptBox[
         OverscriptBox["S", "^"], 
         RowBox[{"2", "\[InvisibleSpace]", "i"}]], ",", 
        RowBox[{
         RowBox[{"-", "J"}], 
         SubscriptBox[
          OverscriptBox["S", "^"], 
          RowBox[{"1", "\[InvisibleSpace]", "j"}]], 
         SubscriptBox[
          OverscriptBox["S", "^"], 
          RowBox[{"2", "\[InvisibleSpace]", "j"}]]}]}], "]"}], 
      "\[RightAngleBracket]"}]}]}], TraditionalForm]],
  InitializationCell->True,ExpressionUUID->
  "a809f9f0-405d-4864-aba4-c94e0847e4c5"],
 "\n\t\t   ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     FractionBox["i", "\[HBar]"], "J", 
     RowBox[{"\[LeftAngleBracket]", 
      RowBox[{
       SubscriptBox[
        OverscriptBox["S", "^"], 
        RowBox[{"1", "\[InvisibleSpace]", "j"}]], "[", 
