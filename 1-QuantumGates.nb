Cell[TextData[{
 "The ZZ Ising interaction gate is a unitary transformation\n\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["R",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "1"], "(", 
     UnderscriptBox[
      StyleBox["\[Theta]",
       FontWeight->"Bold",
       FontSlant->"Plain"], "_"], ")"}], "=", 
    RowBox[{
     RowBox[{"exp", "\[InvisibleSpace] ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"-", 
         FractionBox[
          RowBox[{"i", "\[InvisibleSpace]", "\[Theta]"}], "2"]}], 
        SubscriptBox[
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["Z",
            FontWeight->"Bold",
            FontSlant->"Plain"], "_"], "_"], "1"], 
        SubscriptBox[
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["Z",
            FontWeight->"Bold",
            FontSlant->"Plain"], "_"], "_"], "2"]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{"exp", "\[InvisibleSpace] ", 
       FractionBox["1", "2"], 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"-", "i"}], "0", "0", "0"},
          {"0", 
           RowBox[{"+", "i"}], "0", "0"},
          {"0", "0", 
           RowBox[{"+", "i"}], "0"},
          {"0", "0", "0", 
           RowBox[{"-", "i"}]}
         }], ")"}]}], "=", 
      RowBox[{"exp", "(", "\[InvisibleSpace]", 
       RowBox[{"\[Theta]", 
        SubscriptBox[
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["A",
