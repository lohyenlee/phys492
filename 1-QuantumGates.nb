       FontWeight->"Bold",
       FontSlant->"Plain"], "_"], "_"], "2"], TraditionalForm]],
  ExpressionUUID->"0abdb6b4-168e-403b-a352-150ac9ae7047"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     UnderscriptBox[
      UnderscriptBox[
       StyleBox["P",
        FontWeight->"Bold",
        FontSlant->"Plain"], "_"], "_"], "1"], "(", "\[Theta]", ")"}], 
   TraditionalForm]],ExpressionUUID->"11d18561-3766-46bf-89fe-b726a737465c"],
 ", ",
