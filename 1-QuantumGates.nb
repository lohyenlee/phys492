Cell[TextData[{
 "The operator for a rotation by angle\t\[Phi] about the +Z axis can be \
written in terms of each of these quantities as follows:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    UnderscriptBox[
     UnderscriptBox[
      StyleBox["U",
       FontWeight->"Bold",
       FontSlant->"Plain"], "_"], "_"], "=", 
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", "\[InvisibleSpace]", 
        FractionBox[
         RowBox[{"i", "\[InvisibleSpace]", "\[Phi]"}], "\[HBar]"]}], 
       SubscriptBox[
        UnderscriptBox[
         UnderscriptBox[
          StyleBox["S",
           FontWeight->"Bold",
           FontSlant->"Plain"], "_"], "_"], 
        StyleBox["Z",
         FontSlant->"Plain"]]}], ")"}], "=", 
     RowBox[{
      RowBox[{"exp", "(", 
       RowBox[{
        RowBox[{"-", "\[InvisibleSpace]", 
         FractionBox[
          RowBox[{"i", "\[InvisibleSpace]", "\[Phi]"}], "2"]}], 
        SubscriptBox[
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["\[Sigma]",
            FontWeight->"Bold",
            FontSlant->"Plain"], "_"], "_"], 
         StyleBox["Z",
          FontSlant->"Plain"]]}], ")"}], "=", 
      RowBox[{
       RowBox[{"exp", "(", 
        RowBox[{"\[Phi]", 
         SubscriptBox[
          UnderscriptBox[
           UnderscriptBox[
            StyleBox["A",
             FontWeight->"Bold",
             FontSlant->"Plain"], "_"], "_"], 
          StyleBox["Z",
           FontSlant->"Plain"]]}], ")"}], "=", 
       RowBox[{"exp", "(", 
        RowBox[{
         FractionBox["\[Phi]", "2"], 
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["k",
            FontWeight->"Bold",
            FontSlant->"Plain"], "_"], "_"]}], ")"}]}]}]}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f9dd0cc7-87d7-4d3b-8659-075b2e35319a"],
