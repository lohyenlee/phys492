Cell[TextData[{
 "This is related to the fact that one can construct ",
 StyleBox["finite",
  FontSlant->"Italic"],
 " Z-rotations by combining ",
 StyleBox["finite",
  FontSlant->"Italic"],
 " X- and Y-rotations:",
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "X"], "(", "\[Pi]", ")"}], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "Y"], "(", "\[Pi]", ")"}]}], "=", 
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["R",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "Z"], "(", "\[Pi]", ")"}]}], 
   TraditionalForm]],ExpressionUUID->"b275b988-0220-4fba-8c4b-cfd3d3bfd1d2"],
 ".\n"
}], "Text",
