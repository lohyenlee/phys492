             SubscriptBox["\[Theta]", "x"]}]}], "\[Theta]"], "sin", " ", 
          FractionBox["\[Theta]", "2"]}]},
        {"0", "0", 
         RowBox[{
          FractionBox[
           RowBox[{
            SubscriptBox["\[Theta]", "y"], "-", 
            RowBox[{"i", "\[InvisibleSpace]", 
             SubscriptBox["\[Theta]", "x"]}]}], "\[Theta]"], "sin", " ", 
          FractionBox["\[Theta]", "2"]}], 
         RowBox[{
          RowBox[{"cos", " ", 
           FractionBox["\[Theta]", "2"]}], "+", 
          RowBox[{
           FractionBox[
            RowBox[{"i", "\[InvisibleSpace]", 
             SubscriptBox["\[Theta]", "z"]}], "\[Theta]"], "sin", " ", 
           FractionBox["\[Theta]", "2"]}]}]}
       }], ")"}]}]}], TraditionalForm]],ExpressionUUID->
