Cell[TextData[{
 "Earlier, we saw that one may use H and P gates to construct an arbitrary \
rotation gate R(\[Alpha],\[Beta],\[Gamma]).  By the same logic, one may use \
CP gates (on qubits 1 and 2) together with H and P gates (on qubit 2) to \
construct an arbitrary controlled rotation (CR) gate:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["R",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "Z12"], "(", "\[Theta]", ")"}], "=", 
    RowBox[{
     SuperscriptBox["e", 
      RowBox[{
       RowBox[{"-", "i"}], "\[InvisibleSpace]", 
       RowBox[{"\[Theta]", "/", "2"}]}]], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["P",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "12"], "(", "\[Theta]", ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"10dcd43e-4696-4e2b-8a5b-70c6ed7ffd2f"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["R",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "X12"], "(", "\[Theta]", ")"}], "=", 
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["H",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "2"], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "Z12"], "(", "\[Theta]", ")"}], 
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["H",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "2"]}]}], TraditionalForm]],
  ExpressionUUID->"04ab6b63-def8-4435-9dec-28725de923a7"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["R",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "Y12"], "(", "\[Theta]", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "Z2"], "(", 
      FractionBox["\[Pi]", "2"], ")"}], 
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["H",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "2"], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "Z2"], "(", "\[Theta]", ")"}], 
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["H",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "2"], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "Z2"], "(", 
      RowBox[{"-", 
       FractionBox["\[Pi]", "2"]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"620ac317-3baf-4a4b-aaff-72a9a69ff66d"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["R",
         FontWeight->"Bold",
