Cell[TextData[{
 "Conversely, if one is able to implement a ZZ interaction gate, one can turn \
this into a CP gate:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["P",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "12"], "(", 
     RowBox[{
      RowBox[{"-", "2"}], "\[InvisibleSpace]", "\[Theta]"}], ")"}], "=", 
    RowBox[{
     SuperscriptBox["e", 
      RowBox[{"i", "\[InvisibleSpace]", 
       RowBox[{"\[Theta]", "/", "2"}]}]], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["P",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "1"], "(", "\[Theta]", ")"}], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["P",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "2"], "(", "\[Theta]", ")"}], 
     RowBox[{
      SubscriptBox[
       UnderscriptBox[
        UnderscriptBox[
         StyleBox["R",
          FontWeight->"Bold",
          FontSlant->"Plain"], "_"], "_"], "ZZ"], "(", "\[Theta]", ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"4f85eac2-f62d-41a2-8d21-edc84365f188"],
 "."
}], "Text",
 CellChangeTimes->{{3.896386857552652*^9, 
  3.89638692807098*^9}},ExpressionUUID->"ec61de18-6591-4ff5-b258-\
121f6f497787"]
