Cell[TextData[{
 "Now consider the Heisenberg \[OpenCurlyDoubleQuote]SS\
\[CloseCurlyDoubleQuote] gate, which results from allowing two spins to \
interact via a SU(2)-symmetric exchange coupling:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      UnderscriptBox[
       UnderscriptBox[
        StyleBox["R",
         FontWeight->"Bold",
         FontSlant->"Plain"], "_"], "_"], "SS"], "(", "\[Theta]", ")"}], "=", 
    
    RowBox[{
     RowBox[{"exp", "(", 
      RowBox[{
       RowBox[{"-", 
        FractionBox[
         RowBox[{"i", "\[InvisibleSpace]", "\[Theta]"}], "2"]}], 
       RowBox[{
        SubscriptBox[
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["S",
            FontWeight->"Bold",
            FontSlant->"Plain"], "_"], "_"], "1"], 
        StyleBox["\[CenterDot]",
         FontWeight->"Bold"], 
        SubscriptBox[
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["S",
            FontWeight->"Bold",
            FontSlant->"Plain"], "_"], "_"], "2"]}]}], ")"}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          SuperscriptBox["e", 
           RowBox[{
            RowBox[{"-", "i"}], "\[InvisibleSpace]", 
            RowBox[{"\[Theta]", "/", "2"}]}]], "0", "0", "0"},
         {"0", 
          RowBox[{
           SuperscriptBox["e", 
            RowBox[{"i", "\[InvisibleSpace]", 
             RowBox[{"\[Theta]", "/", "2"}]}]], "cos", " ", "\[Theta]"}], 
          RowBox[{
           RowBox[{"-", "i"}], "\[InvisibleSpace]", 
           SuperscriptBox["e", 
            RowBox[{"i", "\[InvisibleSpace]", 
             RowBox[{"\[Theta]", "/", "2"}]}]], "sin", " ", "\[Theta]"}], "0"},
         {"0", 
          RowBox[{
           RowBox[{"-", "i"}], "\[InvisibleSpace]", 
           SuperscriptBox["e", 
            RowBox[{"i", "\[InvisibleSpace]", 
             RowBox[{"\[Theta]", "/", "2"}]}]], "sin", " ", "\[Theta]"}], 
          RowBox[{
           SuperscriptBox["e", 
            RowBox[{"i", "\[InvisibleSpace]", 
             RowBox[{"\[Theta]", "/", "2"}]}]], "cos", " ", "\[Theta]"}], "0"},
         {"0", "0", "0", 
          SuperscriptBox["e", 
           RowBox[{
            RowBox[{"-", "i"}], "\[InvisibleSpace]", 
            RowBox[{"\[Theta]", "/", "2"}]}]]}
        }], ")"}], "=", 
      RowBox[{"exp", "(", "\[InvisibleSpace]", 
       RowBox[{"\[Theta]", 
        SubscriptBox[
         UnderscriptBox[
          UnderscriptBox[
           StyleBox["A",
            FontWeight->"Bold",
            FontSlant->"Plain"], "_"], "_"], "SS"]}], ")"}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"72f02e0c-6706-43f6-a4b1-1fb8ad40da92"],
